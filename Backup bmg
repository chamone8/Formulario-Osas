<script>
      DocumentoVisualizacaoForm.camposCarregados(function () {
          var $modal = $(document.body).find('.modal-content');
          $modal.find("#CAMPO_18").click();

        Ecm.registraEvento("change", "#CAMPO_2", function () {
         //  $("#CAMPO_18 option:first").attr('selected','selected');
			$("#CAMPO_18").trigger('change');
           });
        Ecm.registraEvento("change", "#CAMPO_18", function () {
               //$(document.body).find($modal).on('change', $modal.find('#CAMPO_18'), conteudo);
               $("#CAMPO_18").load();
               conteudo();
           });
        function conteudo(){
                    var item = $("#CAMPO_18").val();
                    if(item == "HONORARIOS"){
                      $modal.find(".comum").show();
                      $modal.find(".assessoria").show();
                      $modal.find(".honorario").show();
                      $modal.find(".convenio").hide();
                    }
                    else if(item == "COMISSAO"){
                      $modal.find(".comum").show();
                      $modal.find(".assessoria").show();
                      $modal.find(".honorario").hide();
                      $modal.find(".convenio").hide();
                    }
                    else if(item == "PAGTO-CONVENIOS"){
                      $modal.find(".comum").show();
                      $modal.find(".convenio").show();
                      $modal.find(".assessoria").hide();
                      $modal.find(".honorario").hide();
                    }
           }
		});
</script></body>
